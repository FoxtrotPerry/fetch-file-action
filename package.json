{
  "name": "typescript-action",
  "type": "module",
  "description": "GitHub Actions TypeScript template",
  "version": "0.0.0",
  "author": "",
  "private": true,
  "homepage": "https://github.com/FoxtrotPerry/fetch-file-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FoxtrotPerry/fetch-file-action.git"
  },
  "bugs": {
    "url": "https://github.com/FoxtrotPerry/fetch-file-action/issues"
  },
  "keywords": [
    "actions",
    "file",
    "fetch",
    "download"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "vitest --run",
    "coverage": "make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "npx eslint . -c ./.github/linters/.eslintrc.yml",
    "package": "ncc build src/index.ts --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "test": "vitest",
    "test:once": "vitest --run",
    "all": "npm run format:write && npm run lint && npm run test:once && npm run coverage && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.5.4",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.3.0",
    "prettier-eslint": "^16.3.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
